{
    "extends": "./tsconfig.json",
    "compilerOptions": {
        "outDir": "build/www/app",
        "module": "es2015",
        "target": "es5",
        "baseUrl": "./",
        "types": ["jest", "node"],
        "allowJs": true,
        "paths": {
            "@angular/*": ["../node_modules/@angular/*"],
            "app/*": ["src/main/webapp/app/*"]
        }
    },
    "files": ["src/test/javascript/spec/entry.ts"],
    "include": ["src/test/javascript/spec/code-editor/*.ts"]
}
